@model BSD_ECOM.ViewModel.IndexViewModel
@{
    ViewData["Title"] = "shippingreturn";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<main class="main">
     <div class="page-header mt-30 mb-50">
        <div class="container-fluid">
            <div class="archive-header">
                <div class="row align-items-center">
                    <div class="col-xl-6">
                        <h1 class="mb-15"> Shipping & Return Policy  </h1>
                        <div class="breadcrumb">
                            <a href="/Home/Index" rel="nofollow"><i class="fi-rs-home mr-5"></i>Home</a>
                             <span></span>  Shipping & Return
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>
    </div>
    <section class="subscription">
        <div class="container">
            <div id="divAboutus" class="divterms">
                <h2 class="text-center text-uppercase heading">
                    
                </h2>
            </div>
            @if (Model.shippingreturns.Count > 0)
            {
                @foreach (var item in Model.shippingreturns)
                {
                        @Html.Raw(item.shipping)
                }
            }
        </div>
    </section>
</main>

