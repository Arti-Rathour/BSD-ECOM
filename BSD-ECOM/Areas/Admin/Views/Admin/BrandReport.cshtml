
@{
    ViewData["Title"] = "BrandReport";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}
<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <div class="content-wrapper">
        <div class="container-full">
            <div class="content-header">
                <div class="d-flex align-items-center">
                    <div class="mr-auto">
                        <h3 class="page-title" id="brandreport">Brand/Item wise Orders</h3>
                        <div class="d-inline-block align-items-center">
                            <nav>
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="#"><i class="mdi mdi-home-outline"></i></a></li>
                                    <li class="breadcrumb-item" aria-current="page">Brand Report</li>
                                </ol>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
            <!--Main content-->
            <section class="content">
                <div class="row">
                    <div class="col-12">
                        <div class="box">
                            <div class="box-body">
                                <form action="#">
                                    <div class="form-body">
                                        <input type="hidden" id="reg_id" value="0" />
                                        <div class="row">
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label class="font-weight-700 font-size-16">Name</label>
                                                    <input type="text" class="form-control" id="txtName" placeholder="Name" name="first_name" onchange="ReTableBind()" />
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label class="font-weight-700 font-size-16">Mobile No</label>
                                                    <input type="text" class="form-control" id="txtMobile" placeholder="mobile" name="mobile" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <input type="button" class="btn btn-danger" value="Export To excel" name="LoadExcel" id="LoadExcel" onclick="exportexcel('xlsx')" />
                                            <input type="button" class="btn btn-danger" value="Search" onclick="Searchbyname()" />
                                            <input type="button" class="btn btn-success" value="Clear" onclick="ClearData()" />
                                        </div>
                                        <div class="table-responsive rounded card-table">
                                            <table class="table border-no" id="data-table">
                                                <thead>
                                                    <tr>
                                                        <th style="display:none">Id</th>
                                                        <th>Name</th>
                                                        <th>Address</th>
                                                        <th>Mobile No</th>
                                                        <th>Brand/Item</th>
                                                        <th>Brand</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="tbodyData"></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!--/ Content-->
        </div>
    </div>
    <script src="@Url.Content("~/js/Admin/BrandReport.js")"></script>
    <script src="@Url.Content("~/js/xlxs.full.min.js")"></script>
    @*<script type="text/javascript">
            ShowDataInTableBrandReport();
        </script>*@
</body>
</html>
