
@{
    ViewData["Title"] = "Order_Delivery_Report";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}



<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <div class="content-wrapper">
        <div class="container-full">
            <div class="content-header">
                <div class="d-flex align-items-center">
                    <div class="mr-auto">
                        <h5 class="page-title">Delivered Orders Report</h5>
                        <div class="d-inline-block align-items-center">
                            <nav>
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="#"><i class="mdi mdi-home-outline"></i></a></li>
                                    <li class="breadcrumb-item" aria-current="page">Delivered Orders</li>
                                </ol>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
            <!--Main content-->
            <section class="content">
                <div class="row">
                    <div class="col-12">
                        <div class="box">
                            <div class="box-body row">
                                <div class="col-md-12" style="margin-bottom:20px;">
                                    <form action="#">
                                        <div class="form-body">
                                            <div id="divDescription">
                                                <div class="row">
                                                    <input type="hidden" id="order_id" />
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="font-weight-700 font-size-16"> From Date </label>
                                                            <input type="date" class="form-control" id="txtfdate" />
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="font-weight-700 font-size-16"> To Date </label>
                                                            <input type="date" class="form-control" id="txttdate" />
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="font-weight-700 font-size-16">User Name</label>
                                                            <select id="ddlUserNames" class="form-control" tabindex="1" name="UserName" onchange="SearchReturned_Orders();">
                                                                <option value="0">Select UserNane</option>
                                                                @if (ViewBag.ItemUserNane != null)
                                                                {
                                                                    foreach (var item in ViewBag.ItemUserNane as List<SelectListItem>)
                                                                    {
                                                                        <option value="@item.Value">@item.Text</option>
                                                                    }
                                                                }
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    
                                    </form>
                                </div>
                                <div class="col-md-12">
                                    <table id="data-table" class="table table-bordered">
                                        <thead>
                                            <tr>
                                                <th>Order No</th>
                                                <th>Customer</th>
                                                <th>Amount</th>
                                                <th>Mobile No</th>
                                                <th>Order Date</th>
                                                <th>Dispatch Date</th>
                                                <th>Payment Type</th>
                                               
                                            </tr>
                                        </thead>
                                        <tbody class="Delivered_Ordersbody"></tbody>
                                    </table>
                                   
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            
        </div>
    </div>
    
  

</body>
</html>

<script src="~/js/Admin/order_delivered_report.js"></script>
