
@{
    ViewData["Title"] = "Vendor_Mapping";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h1>Vendor_Mapping</h1>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <div class="content-wrapper">
        <div class="container-full">
            <div class="content-header">
                <div class="d-flex align-items-center">
                    <div class="mr-auto" style="margin-top: -136px;">
                        <h3 class="page-title">Vendor Mapping</h3>
                        <div class="d-inline-block align-items-center">
                            <nav>
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="#"><i class="mdi mdi-home-outline"></i></a></li>
                                    <li class="breadcrumb-item" aria-current="page">Vendor Mapping</li>
                                </ol>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
            <!--Main content-->
            <section class="content" style="    margin-top: -35px;">
                <div class="row">
                    <div class="col-12">
                        <div class="box">
                            <div class="box-body">
                                <form action="#">
                                    <div class="form-body">
                                        <input type="hidden" id="id" value="0" />
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="font-weight-700 font-size-16" for="txtvendername">Vendor</label>

                                                    <select id="ddldistrict" class="form-control form-controls" onchange="ProductList56(),BindReport(@ViewBag.TypeId)">
                                                        <option value="0">Select</option>
                                                        @if (ViewBag.District != null)
                                                        {
                                                            foreach (var item in ViewBag.District as List<SelectListItem>)
                                                            {
                                                                <option value="@item.Value">@item.Text</option>
                                                            }
                                                        }
                                                    </select>
                                                </div>
                                            </div>
                                         </div>
                                        <div class="col-md-12">
                                            <input type="button" class="btn btn-primary" value="Export To Excel" onclick="exportexcel()" />

                                        </div>
                                   </div>
                                </form>
                                <div class="table-responsive rounded card-table">
                                    <table class="table border-no" id="example1">
                                        <thead>
                                            <tr>
                                                
                                                <th></th>
                                                <th>id</th>
                                                <th>Product Name</th>
                                                <th>Price</th>
                                                <th>Vendor price</th>
                                            </tr>
                                        </thead>
                                        <tbody class="tbodyData">
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-actions mt-10">
                                    <input type="button" class="btn btn-primary" id="btnMapped" onclick="VendorMapping()" value="Mapped" />
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!--/ Content-->
            <!--/ Content-->
        </div>
    </div>
    <script src="@Url.Content("~/js/Admin/Vendor_mapping.js")"></script>
    <script src="@Url.Content("~/js/Admin/xlxs.full.min (1).js")"></script>
    <script type="text/javascript">
        ProductList56();
    </script>
</body>
</html>