
@{
    ViewData["Title"] = "OR_to_Deliver";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <div class="content-wrapper">
        <div class="container-full">
            <div class="content-header">
                <div class="d-flex align-items-center">
                    <div class="mr-auto">
                        <h3 class="page-title">Orders Ready to Deliver</h3>
                        <div class="d-inline-block align-items-center">
                            <nav>
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="#"><i class="mdi mdi-home-outline"></i></a></li>
                                    <li class="breadcrumb-item" aria-current="page">Orders Ready to Deliver</li>
                                </ol>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
            <!--Main content-->
            <section class="content">
                <div class="row">
                    <div class="col-12">
                        <div class="box">
                            <div class="box-body">
                                <form action="#">
                                    <div class="form-body">
                                        <div id="divDescription">

                                            <div class="row">
                                                <input type="hidden" id="order_id" value="0" />
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label class="font-weight-700 font-size-16"> From Date </label>
                                                         <input type="date" class="form-control" id="txtfdate"  />
                                                    </div>
                                                </div>

                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label class="font-weight-700 font-size-16"> To Date </label>
                                                         <input type="date" class="form-control" id="txttdate"  />
                                                    </div>
                                                </div>

                                              
                                            </div>
                                               
                                                                                       
                                      </div>
                                    </div>
                                    <div class="form-actions mt-10">
                                        <input type="button" class="btn btn-primary" id="btnSave" onclick="SearchOR_to_Deliver();"
                                               value="Serach" />
                                        <input type="button" class="btn btn-danger" value="Clear" onclick="Clear()" />
                                    </div>
                                </form>
                                <div class="col-md-12">
                                    <table id="data-table" class="table table-bordered">
                                        <thead>
                                            <tr>
                                                 <th>Order No</th> 
                                                 <th>Customer</th>
                                                 <th>Amount</th>  
                                                 <th>Mobile No</th>  
                                                <th>Order Date</th>
                                                <th>Dispatch Date</th>
                                                <th>Payment Type</th>
                                                <th>view</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody class="Orderdiliverbody"></tbody>
                                    </table>
                                    <div class="form-actions mt-10" style="text-align:right">
                               <input type="button" class="btn btn-primary" id="btnUpdate" name="btnUpdate" onclick="ApproveOR_to_Deliver();" value="UPDATE" />
                                </div>
                            </div>
                        </div>


                    </div>


                </div>
            </section>
        </div>
    </div>
   

    <script src="@Url.Content("~/js/Admin/Orders.js")"></script>
    <script type="text/javascript">
         ShowOR_to_Deliver();
         //SearchOR_to_Deliver();
    </script>

</body>
</html>



