
@{
    ViewData["Title"] = "CompanyMasters";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}
<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <div class="content-wrapper">
        <div class="container-full">
            <div class="content-header">
                <div class="d-flex align-items-center">
                    <div class="mr-auto">
                        <h3 class="page-title">Printing Setup</h3>
                        <div class="d-inline-block align-items-center">
                            <nav>
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="#"><i class="mdi mdi-home-outline"></i></a></li>
                                    <li class="breadcrumb-item" aria-current="page">Printing Setup</li>
                                </ol>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
            <!--Main content-->
            <section class="content">
                <div class="row">
                    <div class="col-12">
                        <div class="box">
                            <div class="box-body">
                                <form action="#">
                                    <div class="form-body">
                                        <input type="hidden" id="Comp_Id" value="0" />
                                        @*<input type="hidden" id="compId" name="Comp_Id" />*@
                                        @*<input type="hidden" id="gline1" name="gline1" />
                                            <input type="hidden" id="gline2" name="gline2" />*@
                                        <div class="row">
                                            <div class="col-md-3 pr-1">
                                                <div class="row form-group">
                                                    <label>Printing Type <span style="color:red">*</span></label>
                                                    <div class="col-md-12">
                                                        <select class="form-control" id="ddlPrintingId">
                                                            <option>--Select Printing Master--</option>
                                                            @if (ViewBag.printingList != null)
                                                            {
                                                                foreach (var item in ViewBag.printingList as List<SelectListItem>)
                                                                {
                                                                    <option value="@item.Value">@item.Text</option>
                                                                }
                                                            }
                                                        </select>
                                                        <span asp-validation-for="printingType" class="text-danger"></span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3 pr-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>Domain Name</label>
                                                        <input type="text" class="form-control" id="txtdomainname" name="domainname">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3 pr-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>Company Name</label>
                                                        <input type="text" class="form-control" id="txtCompName" name="CompanyName">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3 px-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>Short Name</label>
                                                        <input type="text" class="form-control" id="txtshortName" name="ShortName">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3 px-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>Email Address</label>
                                                        <input type="email" class="form-control" id="txtEmail" name="EmailID" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-2 px-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>Phone No</label>
                                                        <input type="text" class="form-control" id="txtPhone" name="PhoneNo" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-2 px-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>Mobile Number</label>
                                                        <input type="text" class="form-control" id="txtMobile" name="MobileNo" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3 px-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>Address</label>
                                                        <input type="text" class="form-control" id="txtAddress" name="Comp_Address" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-2 px-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>Pin Code</label>
                                                        <input type="text" class="form-control" id="txtPinCode" name="PinCode" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3 px-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>City Name</label>
                                                        <input type="text" class="form-control" id="txtCity" name="CityName" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-2 px-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>Fax No</label>
                                                        <input type="text" class="form-control" id="txtFax" name="FaxNo" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-2 px-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>GST No</label>
                                                        <input type="text" class="form-control" id="txtGst" name="GstNo" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3 px-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>Website</label>
                                                        <input type="text" class="form-control" id="txtWebsite" name="webs" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-2 px-1">
                                                <div class="row form-group">
                                                    <div class="col-md-12">
                                                        <label>TIN NO</label>
                                                        <input type="text" class="form-control" id="txtTinNo" name="TinNo" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="font-weight-700 font-size-16">Logo</label>
                                                    <input type="file" class="form-control" id="imagefile" name="Logo" />
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <img id="img" src="~/images/dummy-pic.png" alt="your image" width="75" height="80" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="font-weight-700 font-size-16">Status</label>
                                                    <input type="checkbox" id="chkStatus" checked name="comp_status" />Active
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-actions mt-10">
                                        <input type="button" class="btn btn-primary" id="btnSave" onclick="Save()" value="Save" />
                                        <input type="button" class="btn btn-danger" value="Clear" onclick="Clear()" />
                                    </div>
                                </form>
                                <div class="col-md-12">
                                    <table class="table border-no" id="example1">
                                        <thead>
                                            <tr>
                                                <th style="display:none">comp_Id</th>
                                                <th>Company Name</th>
                                                <th>Short Name</th>
                                                <th>Domain Name</th>
                                                <th>Logo</th>
                                                <th>Email</th>
                                                <th>Phone Number</th>
                                                <th>Address</th>
                                                <th>Mobile Number</th>
                                                <th>Website</th>
                                                <th>Status</th>
                                                <th>Edit</th>
                                                <th>Delete</th>
                                            </tr>
                                        </thead>
                                        <tbody class="Comptbody"></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!--/ Content-->
        </div>
    </div>
    <script src="@Url.Content("~/js/Admin/CompanyMasters.js")"></script>
    <script>
        ShowDataInTable();
    </script>
</body>
</html>
